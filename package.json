{
  "name": "g2i-node-test",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "esnext",
  "description": "G2i's Node code challenge",
  "repository": {
    "url": "https://gist.github.com/gonzachr"
  },
  "scripts": {
    "start": "npm run build && NODE_ENV=production node dist/src/server.js",
    "dev": "NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "test": "NODE_ENV=test jest --config jest.config.json --watch --detectOpenHandles --testTimeout=10000 --runInBand",
    "build": "tsc",
    "populate": "NODE_ENV=development ts-node src/scripts/populateDb",
    "genTest": "ts-node src/scripts/genTest.ts"
  },
  "author": "Gonzalo Rodriguez",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-redis": "^5.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "ioredis": "^4.17.3",
    "pg": "^8.3.3",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.26",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "0.0.14",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/ioredis": "^4.17.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/supertest": "^2.0.10",
    "@types/yup": "^0.29.7",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.3"
  }
}
